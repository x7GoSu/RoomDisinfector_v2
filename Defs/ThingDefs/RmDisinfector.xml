<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="BuildingBase">
    <defName>Disinfection_Stand</defName>
    <label>Disinfectant Stand</label>
    <description>Disinfects the area.</description>
    <graphicData>
      <texPath>Things/Building/Misc/VitalsMonitor</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
    </graphicData>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToBuild>6000</WorkToBuild>
      <Mass>20</Mass>
      <Flammability>0.7</Flammability>
      <Cleanliness>0</Cleanliness>
    </statBases>
    <size>(1,1)</size>
    <costList>
      <Steel>0</Steel>
      <ComponentIndustrial>0</ComponentIndustrial>
    </costList>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>200</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="RoomDisinfector_v2.CompProperties_PowerCleanliness">
        <secondaryCleanliness>100</secondaryCleanliness>
      </li>
    </comps>
    <designationCategory>Misc</designationCategory>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsMisc</li>
    </thingCategories>
     </ThingDef>

</Defs>
